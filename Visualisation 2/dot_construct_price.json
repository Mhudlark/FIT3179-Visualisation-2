{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A scatterplot showing horsepower and miles per gallons for various cars.",
  "width": 600,
  "height": 400,
  "background": "#f5f5f5",
  "data": {
    "url": "data/airbnb.csv"
  },
  "mark": "circle",
  "encoding": {
    "size": {
      "value": 200
    },
    "x": {
      "field": "construction_year",
      "type": "temporal",
      "title": "Year of construction"
    },
    "y": {
      "aggregate": "mean",
      "field": "price",
      "type": "quantitative",
      "title": "Average Price"
    }
  }
}
