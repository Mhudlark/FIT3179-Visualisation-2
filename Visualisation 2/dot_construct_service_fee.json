{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 400,
  "background": "#f5f5f5",
  "data": {
    "url": "data/airbnb.csv"
  },
  "mark": "circle",
  "encoding": {
    "size": {
      "value": 200
    },
    "x": {
      "field": "construction_year",
      "type": "temporal",
      "title": "Year of construction"
    },
    "y": {
      "aggregate": "mean",
      "field": "service_fee",
      "type": "quantitative",
      "title": "Average Service Fee"
    }
  }
}
